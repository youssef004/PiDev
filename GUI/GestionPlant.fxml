         <image>
            <Image url="@../img/icon_stock.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="54.0" fitWidth="49.0" layoutX="7.0" layoutY="324.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/icon_back.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="59.0" fitWidth="119.0" layoutX="7.0" layoutY="3.0" opacity="0.5" pickOnBounds="true" preserveRatio="true">
