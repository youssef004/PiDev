<AnchorPane id="AnchorPane" prefHeight="0.0" prefWidth="604.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.GestionPlantController">
    <stylesheets>
        <URL value="@gestionplant.css" />
    </stylesheets>
   <children>
      <ImageView fx:id="background_gestion_plant" fitHeight="374.0" fitWidth="604.0" layoutY="4.0" pickOnBounds="true">
