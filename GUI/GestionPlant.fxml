            <Font size="25.0" />
         </font>
      </Button>
      <Button fx:id="RetourGestionProduit" layoutX="32.0" layoutY="334.0" mnemonicParsing="false" onAction="#RetourGestionProduit" prefHeight="27.0" prefWidth="94.0" text="Back">
