            <Image url="@../img/background.png" />
         </image>
      </ImageView>
      <Button fx:id="AjouterPlant" layoutX="70.0" layoutY="107.0" mnemonicParsing="false" onAction="#AjouterPlant" prefHeight="54.0" prefWidth="210.0" text="Add Plant">
